x = int(input("vælg firste tal: "))
y = int(input("vælg andet tal: "))
z = input("vælg en ting: ")


if z == "minus" or z == "-":
    print(x-y)

elif z == "plus" or z == "+":
    print(x+y)

elif z == "divider" or z == "/":
    print(x/y)

elif z == "gange" or z == "*":
    print(x*y)

else:
    print("Prøv igen, og vælg + - / eller *.")
